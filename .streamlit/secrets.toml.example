# Copy this file to secrets.toml and fill in real values.
# secrets.toml is gitignored — never commit real credentials.
#
# On Streamlit Cloud, paste these values into the app's Secrets settings
# (Settings → Secrets) rather than using a file.

[gcs_connections.gcs]
credentials_json = "<paste the full JSON service account key here as a string>"
bucket_name      = "your-gcs-bucket-name"
project_id       = "your-gcp-project-id"
